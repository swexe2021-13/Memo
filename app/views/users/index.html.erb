<div id="wrapper">
    <div id="header">
       <%= image_tag 'rogo4.jpg', class: 'rogo' %>
    </div>
        
    <div id="main">
      <div id="menu">
        <p><%= link_to 'ホームへ', diaries_path, class: 'button02' %></p><br>
        <p><%= link_to 'ログアウト',  login_logout_path, class: 'button02' %></p><br>
      </div>
      
      <div id="contents">
        <% @users.each do |e| %>
          <% if session[:login_uid] == e.id %>
            <p>
            <%= e.name %>
            <%= e.email %>
            <%= link_to '削除', user_path(e), method: :delete %>
            </p>
          <% else %>
            <%= e.name %>
            <%= e.email %>
          <% end %>
        <% end %>
      </div>
      
    </div>
    
    <div id="footer">
      (c)copy right
    </div>
</div>


